{
    "version": 2,
    "buildCommand": "pnpm build",
    "outputDirectory": "dist",
    "functions": {
        "dist/main.js": {
            "runtime": "@vercel/node"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/main.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
