{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.ts",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.ts",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "api/index.ts": {
            "maxDuration": 30
        }
    }
}
