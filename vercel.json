{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": ".",
    "installCommand": "npm install",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
