{
    "version": 2,
    "buildCommand": "pnpm run build",
    "outputDirectory": "dist",
    "installCommand": "pnpm install",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/dist/main.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
