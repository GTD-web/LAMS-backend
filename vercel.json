{
    "version": 2,
    "buildCommand": "pnpm build",
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
