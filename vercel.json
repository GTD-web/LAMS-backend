{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/dist/main.js"
        },
        {
            "source": "/(.*)",
            "destination": "/dist/main.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
